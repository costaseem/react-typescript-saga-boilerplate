{
  "presets": [
    "@babel/react",
    "@babel/typescript",
    [ "@babel/env", { "modules": false } ]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "react-hot-loader/babel"
  ],
  "env": {
    "production": {
      "presets": ["minify"]
    },
    "test": {
      "presets": ["@babel/env", "@babel/preset-react"]
    }
  }
}